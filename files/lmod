LMOD_GENERAL (?:- lmod::)\s*username=%{USERNAME_PAT:[lmod][info][username]}, cluster=%{DATA:[lmod][info][cluster]}, jobid=%{DATA:[lmod][info][jobid]}
LMOD_USERLOAD (?:yes|no)
LMOD_LOAD userload=%{LMOD_USERLOAD:[lmod][userload]}, module=%{DATA:[lmod][module][name]}/%{DATA:[lmod][module][version]}, fn=%{GREEDYDATA:[lmod][filename]}
LMOD_CMD cmd=%{DATA:[lmod][command]}, args=%{GREEDYDATA:[lmod][arguments]}
LMOD_MSG %{LMOD_GENERAL}, (?:%{LMOD_LOAD}|%{LMOD_CMD})
